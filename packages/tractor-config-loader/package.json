{
  "name": "tractor-config-loader",
  "version": "0.2.2",
  "author": "Craig Spence <craigspence0@gmail.com>",
  "repository": {
    "type": "git",
    "url": "https://github.com/phenomnomnominal/tractor-config-loader.git"
  },
  "bugs": {
    "url": "https://github.com/phenomnomnominal/tractor-config-loader/issues",
    "email": "craigspence0@gmail.com"
  },
  "main": "dist/index.js",
  "watch": {
    "build": "src/**/*.js"
  },
  "scripts": {
    "prepublish": "npm run build",
    "build": "npm run lint && npm run cover && npm run compile",
    "compile": "babel src/ --out-dir dist --ignore *.spec.js",
    "cover": "istanbul cover --config .istanbulrc.json node_modules/mocha/bin/_mocha -- --opts ./.mocharc src/*.spec.js",
    "coveralls": "cat ./reports/coverage/lcov.info | ./node_modules/coveralls/bin/coveralls.js",
    "lint": "eslint src/**/*.js; true",
    "test": "mocha --opts ./.mocharc src/*.spec.js",
    "watch": "npm-watch"
  },
  "license": "MIT",
  "engines": {
    "node": ">=0.10"
  },
  "devDependencies": {
    "babel-cli": "6.22.2",
    "babel-core": "6.22.1",
    "babel-eslint": "7.1.1",
    "babel-preset-es2015": "6.14.0",
    "babel-preset-stage-0": "6.5.0",
    "babel-register": "6.14.0",
    "chai": "3.5.0",
    "coveralls": "2.11.16",
    "eslint": "3.14.1",
    "istanbul": "1.0.0-alpha.2",
    "mocha": "3.0.2",
    "npm-watch": "0.1.6",
    "sinon": "1.17.7",
    "sinon-chai": "2.8.0"
  },
  "dependencies": {
    "lodash.defaults": "4.2.0",
    "minimist": "1.2.0"
  }
}
