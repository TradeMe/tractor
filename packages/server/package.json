    "@types/node-fetch": "^2.1.6",
    "@types/socket.io": "^2.1.2",
    "@types/socket.io-client": "^1.4.32",
    "node-fetch": "^2.3.0",
    "socket.io-client": "^2.2.0"
  },
  "resolutions": {
    "webdriver-manager": "^13.0.0"
  },
