    "lodash.template": "^4.4.0",
    "monkeypatch": "^1.0.0",
    "porter-stemmer": "^0.9.1",
    "protractor": "^5.3.0",
    "socket.io": "^2.0.4",
    "terminal-link": "^1.2.0"
  },
  "devDependencies": {
    "@tractor/config-loader": "^1.3.0",
    "@tractor/dependency-injection": "^1.3.0",
    "@tractor/error-handler": "^1.3.0",
