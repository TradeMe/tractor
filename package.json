{
  "name": "tractor",
  "version": "0.0.0",
  "private": true,
  "description": "A UI around Protractor to help write E2E tests for Angular applications without needing to know JavaScript",
  "author": "Craig Spence <craigspence0@gmail.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/TradeMe/tractor.git"
  },
  "bugs": {
    "url": "https://github.com/TradeMe/tractor/issues",
    "email": "craigspence0@gmail.com"
  },
  "scripts": {
    "clean": "rimraf reports",
    "clean:node_modules": "rimraf ./packages/*/node_modules && rimraf node_modules",
    "build": "npm run clean && lerna bootstrap && lerna run build && npm run cover",
    "cover": "istanbul cover --config .istanbulrc.json node_modules/.bin/_mocha -- --opts .mocharc 'packages/**/src/**/*.spec.js'",
    "link": "lerna exec npm link && lerna exec rm package-lock.json",
    "pretest:e2e": "npm run webdriver",
    "test:e2e": "protractor ./tractor/protractor.conf.js --baseUrl=\"http://localhost:4444\"",
    "tractor": "tractor start",
    "tractor:init": "tractor init",
    "webdriver": "webdriver-manager update && tractor patch"
  },
  "devDependencies": {
    "@babel/cli": "7.0.0",
    "@babel/core": "7.0.1",
    "@babel/plugin-proposal-export-default-from": "7.0.0",
    "@babel/plugin-proposal-object-rest-spread": "7.0.0-beta.39",
    "@babel/plugin-proposal-optional-catch-binding": "7.0.0-beta.39",
    "@babel/polyfill": "7.0.0-beta.39",
    "@babel/preset-env": "7.0.0-beta.39",
    "@babel/register": "7.0.0-beta.39",
    "@tractor-plugins/mocha-specs": "^0.1.0",
    "@tractor-plugins/page-objects": "^0.5.0",
    "@tractor/cli": "^1.0.1-beta.2",
    "@tractor/unit-test": "^1.0.1-beta.2",
    "babel-eslint": "^8.2.5",
    "eslint": "^5.0.1",
    "istanbul": "^1.1.0-alpha.1",
    "lerna": "^2.11.0",
    "mocha": "^5.2.0",
    "npm-watch": "^0.3.0",
    "rimraf": "^2.6.2"
  }
}
