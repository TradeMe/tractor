{
  "name": "tractor",
  "version": "0.12.0-beta.4",
  "author": "Craig Spence <craig@trademe.co.nz>",
  "repository": {
    "type": "git",
    "url": "https://github.com/TradeMe/tractor.git"
  },
  "bugs": {
    "url": "https://github.com/TradeMe/tractor/issues",
    "email": "craig@trademe.co.nz"
  },
  "watch": {
    "build": "src/**/*.js"
  },
  "scripts": {
    "prepublish": "npm run build",
    "build": "npm run clean && npm run lint && npm run cover && npm run compile",
    "clean": "rimraf dist",
    "compile": "babel src/ --out-dir dist --ignore *.spec.js",
    "cover": "istanbul cover --config .istanbulrc.json node_modules/mocha/bin/_mocha -- --opts ./.mocharc 'src/**/*.spec.js'",
    "lint": "eslint src/**/*.js; true",
    "test": "mocha --opts ./.mocharc 'src/**/*.spec.js'",
    "test:debug": "mocha --inspect --debug-brk --opts ./.mocharc 'src/**/*.spec.js'",
    "watch": "npm-watch"
  },
  "license": "MIT",
  "engines": {
    "node": ">=0.10"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-eslint": "^8.0.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "babel-register": "^6.26.0",
    "chai": "^4.1.2",
    "dirty-chai": "^2.0.1",
    "eslint": "^4.6.1",
    "ineeda": "^0.11.0",
    "istanbul": "1.0.0-alpha.2",
    "mocha": "^3.5.3",
    "npm-watch": "^0.2.0",
    "rimraf": "^2.6.2",
    "sinon": "^4.0.0",
    "sinon-chai": "^2.13.0"
  },
  "dependencies": {
    "bluebird": "^3.5.0",
    "commander": "^2.11.0",
    "protractor": "^5.1.2",
    "tractor-config-loader": "^0.4.0",
    "tractor-dependency-injection": "^0.1.1",
    "tractor-error-handler": "^0.2.2",
    "tractor-file-structure": "^0.5.0",
    "tractor-logger": "^0.1.0",
    "tractor-plugin-loader": "^0.6.0",
    "tractor-server": "^0.12.3"
  },
  "bin": {
    "tractor": "./bin/tractor"
  }
}
