		"porter-stemmer": "^0.9.1",
		"protractor": "^5.3.0",
		"request": "^2.88.0",
		"request-promise-native": "^1.0.7",
		"socket.io": "^2.0.4",
		"socket.io-client": "^2.0.3",
		"text-diff": "^1.0.1",
		"title-case": "^2.1.1",
		"vinyl-buffer": "^1.0.1",
		"vinyl-source-stream": "^2.0.0",
		"xterm": "^3.13.0"
	},
	"nyc": {
		"report-dir": "./reports",
		"reporter": [
			"text",
			"lcov",
			"html"
		],
		"exclude": [
			"**/*.spec.js",
			"**/dist/**"
		],
		"cache": true
	}
}
